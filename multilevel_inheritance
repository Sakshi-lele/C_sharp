using System;

class LivingThing
{
    public void Grow()
    {
        Console.WriteLine("Living things grow");
    }
}

class Animal : LivingThing
{
    public void Move()
    {
        Console.WriteLine("Animal moves");
    }
}

class Dog : Animal
{
    public void Bark()
    {
        Console.WriteLine("Dog Barks!");
    }
}

class Program
{
    static void Main()
    {
        Dog d = new Dog();
        d.Grow();
        d.Move();
        d.Bark();

    }
}
